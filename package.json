    "package.json": "./package.json",
    "readme.mdx": "./README.mdx",
    "no-license.mdx": "./no-license.mdx",
    "no-login.mdx": "./no-login.mdx",
    "src": "./src"
  },
  "dependencies": {
    "@mdx-js/react": "^1",
