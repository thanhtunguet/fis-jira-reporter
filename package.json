    "dayjs": "^1.11.7",
    "firebase": "^9.20.0",
    "i18next": "~20",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "~10.9",
    "react-markdown": "^8.0.7",
    "react3l": "^7.0.0-rc4",
